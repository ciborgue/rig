    target iqn.1999-01.the.miner:default tpgt 1 {
        enable yes
        attribute {
            authentication no
            cache_dynamic_acls yes
            default_cmdsn_depth 64
            default_erl 0
            demo_mode_discovery yes
            demo_mode_write_protect yes
            fabric_prot_type 0
