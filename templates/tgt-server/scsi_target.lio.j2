        block_size 512
        emulate_3pc yes
        emulate_caw yes
        emulate_dpo yes
        emulate_fua_read yes
        emulate_fua_write yes
        emulate_model_alias no
        emulate_rest_reord no
        emulate_tas yes
        emulate_tpu no
        emulate_tpws no
        emulate_ua_intlck_ctrl no
